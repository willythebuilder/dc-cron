{
  "$schema": "../node_modules/wrangler/config-schema.json",
  "name": "dc-sync-chronicles-updates",
  "main": "src/worker.ts",
  "compatibility_date": "2024-11-01",
  "triggers": { "crons": ["*/1 * * * *"] }, // every minute
  "vars": {
    "TARGET_URL": "https://staging-daichronicles.netlify.app/api/internal/sync-chronicles-updates"
  },
  "env": {
    "staging": {
      "triggers": { "crons": ["*/1 * * * *"] },
      "vars": {
        "TARGET_URL": "https://staging-daichronicles.netlify.app/api/internal/sync-chronicles-updates"
      }
    },
    "production": {
      "triggers": { "crons": ["*/1 * * * *"] },
      "vars": {
        "TARGET_URL": "https://daichronicles.io/api/internal/sync-chronicles-updates"
      }
    }
  },
  "observability": { "enabled": true }
}
